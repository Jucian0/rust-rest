   "version": "0.2.0",
   "configurations": [
      {
         "type": "pwa-node",
         "request": "launch",
         "name": "Launch Program",
         "skipFiles": [
            "<node_internals>/**"
         ],
         "program": "${file}"
      },
      {
         "type": "lldb",
         "request": "launch",
         "name": "Launch",
         "args": [],
         "program": "${workspaceFolder}/target/debug/rest-api",
         "windows": {
           "program": "${workspaceFolder}/target/debug/rest-api.d"
         },
         "cwd": "${workspaceFolder}",
         "stopOnEntry": false,
         "sourceLanguages": [
           "rust"
         ]
       }
   ]
}